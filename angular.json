{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "demo-angular",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "demo-angular": {
      "projectType": "application",
      "root": "apps/demo-angular",
      "sourceRoot": "apps/demo-angular/src",
      "prefix": "documentviewer",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/demo-angular",
            "index": "apps/demo-angular/src/index.html",
            "main": "apps/demo-angular/src/main.ts",
            "polyfills": "apps/demo-angular/src/polyfills.ts",
            "tsConfig": "apps/demo-angular/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/demo-angular/src/favicon.ico",
              "apps/demo-angular/src/assets"
            ],
            "styles": ["apps/demo-angular/src/styles.scss"],
            "scripts": ["node_modules/mammoth/mammoth.browser.min.js"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/demo-angular/src/environments/environment.ts",
                  "with": "apps/demo-angular/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo-angular:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-angular:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-angular:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/demo-angular/src/**/*.ts",
              "apps/demo-angular/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/demo-angular"],
          "options": {
            "jestConfig": "apps/demo-angular/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ngx-doc-viewer": {
      "projectType": "library",
      "root": "libs/ngx-doc-viewer",
      "sourceRoot": "libs/ngx-doc-viewer/src",
      "prefix": "documentviewer",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ngx-doc-viewer/src/**/*.ts",
              "libs/ngx-doc-viewer/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ngx-doc-viewer"],
          "options": {
            "jestConfig": "libs/ngx-doc-viewer/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "data": {
      "root": "libs/data",
      "sourceRoot": "libs/data/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/data/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/data"],
          "options": {
            "jestConfig": "libs/data/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "react-docviewer": {
      "root": "libs/react-docviewer",
      "sourceRoot": "libs/react-docviewer/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/react-docviewer/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/react-docviewer"],
          "options": {
            "jestConfig": "libs/react-docviewer/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "demo-react": {
      "root": "apps/demo-react",
      "sourceRoot": "apps/demo-react/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/demo-react",
            "index": "apps/demo-react/src/index.html",
            "main": "apps/demo-react/src/main.tsx",
            "polyfills": "apps/demo-react/src/polyfills.ts",
            "tsConfig": "apps/demo-react/tsconfig.app.json",
            "assets": [
              "apps/demo-react/src/favicon.ico",
              "apps/demo-react/src/assets"
            ],
            "styles": ["apps/demo-react/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/demo-react/src/environments/environment.ts",
                  "with": "apps/demo-react/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "demo-react:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "demo-react:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/demo-react/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/demo-react"],
          "options": {
            "jestConfig": "apps/demo-react/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
